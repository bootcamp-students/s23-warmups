function multi(arr) {
  let result = [1];                          // I don't think this is the right way to do this, but it passed the test for now. 
  for (let i = 0; i < arr.length; i++) {
     result *= arr[i];
  };
  return result;
};

function add(arr) {
  let sum = 0;
  for (let i = 0; i < arr.length; i++) {
    sum += arr[i];
  };
  return sum;
};

function reverse(str) {
  return str.reverse();
}
