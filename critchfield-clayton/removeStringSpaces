/* Create a new variable to store the new string in
    Take a string and treat it as an array of characters
      Use for a loop to go through each character (index) of the string (array)
        If the character does not equal a string, then add it to the new variable
         Return the new string
*/

function noSpace(x){
  let result = '';
 
  for (let i = 0; i < x.length; i++) {
    if(x[i] != ' ') {
      result += x[i];
    }
  }
  return result;
}
